pipeline {
    agent any
    stages {
        stage('checkout') {
            steps {
                git branch : 'DEV', credentialsId : 'github-creds', url: 'https://github.com/philemonboaz/Expense_Tracker_Flutter_SpringBoot.git'
            }
        }
        stage('Build') {
            steps {
                sh 'mvn clean install'
            }
        }
        stage('Archive') {
            steps {
                archiveArtifacts artifacts : 'target/*.jar', fingerprint: true
            }
        }
    }
}